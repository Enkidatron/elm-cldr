{
    "type": "package",
    "name": "enkidatron/elm-cldr",
    "summary": "Locale based date and time formatting",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": [
        "Cldr.Locale",
        "Cldr.Format.Date",
        "Cldr.Format.DateTime",
        "Cldr.Format.Length",
        "Cldr.Format.Options",
        "Cldr.Format.OptionsBuilder"
    ],
    "source-directories": [
        "src"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/browser": "1.0.2 <= v < 2.0.0",
        "elm/core": "1.0.5 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/parser": "1.1.0 <= v < 2.0.0",
        "elm/time": "1.0.0 <= v < 2.0.0",
        "elm-community/string-extra": "4.0.1 <= v < 5.0.0",
        "joneshf/elm-tagged": "2.1.1 <= v < 3.0.0",
        "justinmimbs/date": "4.0.0 <= v < 5.0.0",
        "ryannhg/date-format": "2.3.0 <= v < 3.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}